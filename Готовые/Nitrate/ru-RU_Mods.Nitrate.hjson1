Configs: {
	NitrateConfig: {
		DisplayName: Конфигурация

		ExperimentalTileRenderer: {
			Label: Экспериментальный рендеринг блоков
			Tooltip: Включает экспериментальную систему рендеринга, работающую приблизительно в 5 раз быстрее ванильной, но вызывающую различные визуальные ошибки
		}

		ExperimentalTileRendererWarning: {
			Label: Отключить предупреждение об экспериментальном рендеринге блоков
			Tooltip: Отключает сообщение в чате про экспериментальную систему рендеринга, появляющееся при заходе в мир
		}

		FasterCursor: {
			Label: Улучшенный рендеринг курсора
			Tooltip: Переделывает систему отображения курсора, повышая тем самым его отзывчивость и плавность и убирая привязку к частоте обновления экрана
		}

		UsesNewLaserRulerRendering: {
			Label: Улучшенный рендеринг механической линейки
			Tooltip: Переделывает работу механической линейки, существенно улучшая производительность при её использовании
		}

		UsesAsyncSceneMetrics: {
			Label: Асинхронная проверка блоков вокруг игрока
			Tooltip: Разделяет проверки ближайших к игроку блоков от класса SceneMetrics на несколько кадров, почти полностью убирая нагрузку от него.
		}

		UsesFasterPylonSystem: {
			Label: Улучшенная работа системы пилонов
			Tooltip: Оптимизирует работу пилонов
		}
	}
}

